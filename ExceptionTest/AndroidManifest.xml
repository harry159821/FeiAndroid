<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.njnu.kai.ExceptionTest"
          android:versionCode="2"
          android:versionName="2.1">
    <uses-sdk android:minSdkVersion="5"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application android:name=".KaiApplication" android:label="@string/app_name" android:icon="@drawable/ic_launcher">
        <activity android:name=".KaiActivity"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <service android:name=".KaiService"></service>

        <provider android:name=".KaiProvider" android:authorities="com.njnu.kai.ExceptionTest" />

    </application>
</manifest>
